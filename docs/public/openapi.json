{
  "openapi": "3.1.0",
  "info": {
    "title": "UW Course Map API",
    "version": "1.0.0",
    "description": "Static JSON API for UW-Madison course data",
    "contact": {
      "name": "UW Course Map"
    }
  },
  "servers": [
    {
      "url": "https://api.uwcourses.com",
      "description": "Production API"
    }
  ],
  "tags": [
    {
      "name": "Metadata",
      "description": "API metadata and configuration"
    },
    {
      "name": "Courses",
      "description": "Course information"
    },
    {
      "name": "Instructors",
      "description": "Instructor information"
    },
    {
      "name": "Meetings",
      "description": "Class meetings and schedules"
    },
    {
      "name": "Graphs",
      "description": "Prerequisite graphs"
    },
    {
      "name": "Statistics",
      "description": "Aggregate statistics"
    },
    {
      "name": "Buildings",
      "description": "Campus building data"
    }
  ],
  "paths": {
    "/subjects": {
      "get": {
        "summary": "Get all subject codes and names",
        "operationId": "getSubjects",
        "tags": [
          "Metadata"
        ],
        "responses": {
          "200": {
            "description": "Get all subject codes and names",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/terms": {
      "get": {
        "summary": "Get all term codes and names",
        "operationId": "getTerms",
        "tags": [
          "Metadata"
        ],
        "responses": {
          "200": {
            "description": "Get all term codes and names",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/update": {
      "get": {
        "summary": "Get last update timestamp",
        "operationId": "getUpdate",
        "tags": [
          "Metadata"
        ],
        "responses": {
          "200": {
            "description": "Get last update timestamp",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "updated_on": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/manifest": {
      "get": {
        "summary": "Get API manifest",
        "operationId": "getManifest",
        "tags": [
          "Metadata"
        ],
        "responses": {
          "200": {
            "description": "Lists all available resources for discovery",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "subjects": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "courses": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "instructors": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    },
                    "updated_on": {
                      "type": "string",
                      "format": "date-time"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/course/{courseId}": {
      "get": {
        "summary": "Get course by ID",
        "operationId": "getCourseCourseid",
        "tags": [
          "Courses"
        ],
        "responses": {
          "200": {
            "description": "Get course by ID",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Course"
                }
              }
            }
          },
          "404": {
            "description": "Not found"
          }
        },
        "parameters": [
          {
            "name": "courseId",
            "in": "path",
            "required": true,
            "description": "Course identifier (e.g., COMPSCI_200)",
            "schema": {
              "type": "string"
            },
            "example": "COMPSCI_200"
          }
        ]
      }
    },
